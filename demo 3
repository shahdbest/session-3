public class Demo1 {
                
public static void main(String[] args) {

        //float x=10.5;

        // every decimal val is read as double by def


        float x=(float)10.5; // converting double into float         
        float y=10.5f;// converting double into float 
        
        System.out.println(x);
        System.out.println(y);

        
        int a=10;
        byte b=(byte) 50;
        
}
}
