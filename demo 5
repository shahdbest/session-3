public class Demo1 {

        
        void isCap(char x)
        {        
                if(x>=65 && x<=90)
                        System.out.println(x + " is  is cap");
                else
                        System.out.println(x+" is not cap");        
        }
        
        
public static void main(String[] args) {

        Demo1 demo1=new Demo1();
        demo1.isCap('a');
        demo1.isCap('B');
        demo1.isCap('A');      
        
}

}
